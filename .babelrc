{
  "presets": [
    "react",
    // supports ES6
    // transform-async-to-generator
    "es2017",
    // needed for 
    "es2015",
    // suports arrow funciton and others advances of ES6
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}